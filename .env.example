# Private key of the account used for deployments (DO NOT COMMIT THE ACTUAL KEY)
PRIVATE_KEY=0xYourPrivateKeyHere

# Infura or Alchemy project API key for connecting to testnets/mainnet
INFURA_API_KEY=YourInfuraAPIKey

# Optional: Etherscan API key if you want to verify contracts automatically
ETHERSCAN_API_KEY=YourEtherscanAPIKey

